<%@ page import="java.util.*" %><html>  <head>    <title>Sistema de Tiquetes</title>	<link href="/styles.css" rel="stylesheet" type="text/css"/>  </head>  <body>	<% Map event = (Map)request.getAttribute("event"); %>	<% Map ticket = (Map)request.getAttribute("ticket"); %>	<% Map customer = (Map)request.getAttribute("customer"); %>	<% ArrayList<HashMap> numberedLocations = (ArrayList<HashMap>)request.getAttribute("numberedLocations"); %>	<% String link = (String)request.getAttribute("link");%>	<br/>	<br/>	<br/>	<fieldset align="center">		<legend>Factura de Tiquete</legend>				<table width="100%">						<tr>				<td>Nombre: <%=customer.get("name")%></td>				<td>N&uacute;mero de C&eacute;dula: <%=customer.get("id")%></td>							</tr>						<tr>				<td>Tel&eacute;fono:<%=customer.get("telephone")%></td>				<td>Direci&oacute;n: <%=customer.get("address")%></td>							</tr>						<tr>				<td>Tipo de tarjeta: <%=customer.get("cardType")%></td>				<td>N&uacute;mero de tarjeta: <%=customer.get("cardNumber")%></td>			</tr>					<tr>				<td>Tiquete: <%=ticket.get("code")%></td>				<td>Evento: <%=event.get("name")%></td>			</tr>						<tr>				<td>Fecha de compra: <%=ticket.get("date")%></td>				<td>					<% if (!numberedLocations.isEmpty()) { %>					Numerada: 					<% for (HashMap numberedLocation : numberedLocations) {%>					<%=numberedLocation.get("locationNumber") + " "%>					<% } %>					<% } %>				</td>			</tr>						<tr>				<td>Cantidad: <%=ticket.get("quantity")%></td>				<td>Total a pagar: <%=ticket.get("amount")%></td>			</tr>						<tr>				<td colspan="2" align="center">					<a href="<%= link%>" >						<button>Regresar</button>					</a>				</td>			</tr>					</table>			</fieldset>  </body></html>